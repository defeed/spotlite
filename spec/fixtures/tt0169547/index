                    <button class="btn small" value="Yes" name="ynb_593792_yes" onclick="CS.TMD.user_review_vote(593792, 'tt0169547', 'yes');" >Yes</button>
                    <button class="btn small" value="No" name="ynb_593792_no" onclick="CS.TMD.user_review_vote(593792, 'tt0169547', 'no');" >No</button>
                </div>
