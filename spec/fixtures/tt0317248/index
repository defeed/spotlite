                    <button class="btn small" value="Yes" name="ynb_1308099_yes" onclick="CS.TMD.user_review_vote(1308099, 'tt0317248', 'yes');" >Yes</button>
                    <button class="btn small" value="No" name="ynb_1308099_no" onclick="CS.TMD.user_review_vote(1308099, 'tt0317248', 'no');" >No</button>
                </div>
