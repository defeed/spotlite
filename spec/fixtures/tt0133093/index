                    <button class="btn small" value="Yes" name="ynb_1229286_yes" onclick="CS.TMD.user_review_vote(1229286, 'tt0133093', 'yes');" >Yes</button>
                    <button class="btn small" value="No" name="ynb_1229286_no" onclick="CS.TMD.user_review_vote(1229286, 'tt0133093', 'no');" >No</button>
                </div>
